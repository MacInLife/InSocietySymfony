<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Messagerie" table="messagerie">
    <id name="idMsgrie" type="integer" column="id_msgrie">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="jour" type="date" column="jour" nullable="true"/>
    <field name="heure" type="time" column="heure" nullable="true"/>
    <field name="lieu" type="string" column="lieu" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
